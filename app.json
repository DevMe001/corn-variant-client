{
  "expo": {
		"name": "corn-variety-client",
		"slug": "corn-variety-client",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/adaptive-icon-corn.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/corn-splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#2bc217"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon-corn.png",
				"backgroundColor": "#2bc217"
			},
			"package": "com.cornmaize123.cornvarietydata",
			"permissions": ["android.permission.RECORD_AUDIO"]
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"plugins": [
			[
				"expo-image-picker",
				{
					"photosPermission": "The app accesses your photos to let you share them with your friends.",
					"cameraPermission": "The app accesses your camera to capture an image."
				}
			]
		],
		"extra": {
			"eas": {
				"projectId": "5dceff9e-ef4d-42f8-beaf-a1587d85adf0"
			}
		},
		"owner": "devme"
	}
}
